{
  "confluence": {
    "baseUrl": "${CONFLUENCE_BASE_URL}",
    "email": "${CONFLUENCE_USER_EMAIL}",
    "apiToken": "${CONFLUENCE_API_TOKEN}",
    "spaceKey": "${CONFLUENCE_SPACE_KEY}",
    "maxPages": 1,
    "batchSize": 1,
    "outputDir": "data/confluence-extraction-debug"
  },
  "llm": {
    "apiKey": "${GEMINI_API_KEY}",
    "model": "gemini-2.5-flash",
    "batchSize": 1,
    "outputDir": "data/llm-extraction-debug",
    "maxRetries": 1,
    "delayBetweenRequests": 50
  },
  "validation": {
    "outputDir": "data/validation-debug"
  },
  "pipeline": {
    "steps": ["confluence", "llm"],
    "outputDir": "data/domain-knowledge-debug"
  }
}
